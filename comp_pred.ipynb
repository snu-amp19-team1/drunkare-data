    "\n",
    "#generating the sliding window\n",
    "def sliding_window(data,stepSize,windowSize):\n",
    "    for x in range(0,stepSize):\n",
    "        yield (x, data[x:x + windowSize])\n",
    "    \n",
    "#prediction\n",
    "def load_model(window):\n",
